<configuration>
	<system.web>
		<compilation debug="true" targetFramework="4.0"/>
		<caching>
			<outputCacheSettings>
				<outputCacheProfiles>
					<add name="CacheHourly" duration="120"/>
				</outputCacheProfiles>
			</outputCacheSettings>
		</caching>
	</system.web>
</configuration>
