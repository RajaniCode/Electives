<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    
    <add key="DebugMode" value="True"></add>
    <add key="DataSourceTELEMETRY" value=""></add>
    <add key="IntegratedSecurityTELEMETRY" value=""></add>
    <add key="InitialCatalogTELEMETRY" value=""></add>
    <add key="UsernameTELEMETRY" value=""></add>
    <add key="PasswordTELEMETRY" value=""></add>
    <add key="ConnectionStringTELEMETRY" value=""></add>
    <add key="DataSourceNameTELEMETRY" value=""></add>

    <add key="DataSourceEFM" value=""></add>
    <add key="IntegratedSecurityEFM" value=""></add>
    <add key="InitialCatalogEFM" value=""></add>
    <add key="UsernameEFM" value=""></add>
    <add key="PasswordEFM" value=""></add>
    <add key="ConnectionStringEFM" value=""></add>
    <add key="DataSourceNameEFM" value=""></add>
    
  </appSettings>  
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL" 
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
</configuration>
